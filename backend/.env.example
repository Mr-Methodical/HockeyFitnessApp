# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration - Add your Expo dev server URLs
ALLOWED_ORIGINS=http://localhost:8081,exp://192.168.1.100:8081,exp://localhost:8081

# Optional: Add your production domain when deployed
# ALLOWED_ORIGINS=http://localhost:8081,https://your-app-domain.com
