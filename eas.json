{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_FIREBASE_API_KEY": "AIzaSyDPlTq8FCcKJP8VIqYcY8H4eZd_example",
        "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "hockey-accountability.firebaseapp.com",
        "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "hockey-accountability",
        "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "hockey-accountability.firebasestorage.app",
        "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "123456789",
        "EXPO_PUBLIC_FIREBASE_APP_ID": "1:123456789:android:example"
      },
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./pc-api-key.json",
        "track": "internal"
      }
    }
  }
}
